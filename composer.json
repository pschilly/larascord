{
    "name": "pschilly/larascord",
    "description": "Larascord is a package that allows you to authenticate users in your Laravel application using Discord. This build is based on the original - but heavily modified for usage with JT-HVY website PERSCOM.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jakyeru\\Larascord\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jakyeru\\Larascord\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Schilly",
            "email": "sch1lly.live@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Pschilly\\Larascord\\LarascordServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9"
    },
    "require": {
        "php": "^8.2|^8.3",
        "laravel/framework": "^11",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/breeze": "^v2.0"
    }
}
